<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.1</TargetFramework>
        <LangVersion>latest</LangVersion>

        <Version>1.1.1</Version>
        <Description>Extensions for working with ServiceStack.Aws POCODynamo library for Amazon DynamoDB.</Description>
        
        <Authors>Arinze Anozie</Authors>
        <Company>Aeta Investments Inc.</Company>
        <Copyright>Copyright (c) Aeta Investments Inc.</Copyright>

        <RepositoryType>git</RepositoryType>
        <RepositoryUrl>https://github.com/Aeta-Investments/aeta-extensions-pocodynamo.git</RepositoryUrl>
        <PackageProjectUrl>https://github.com/Aeta-Investments/aeta-extensions-pocodynamo</PackageProjectUrl>
        <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="ServiceStack.Aws" Version="5.11.0" />
    </ItemGroup>

</Project>
